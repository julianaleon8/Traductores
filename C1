using contador,z,y of type integer ; x,w,j of type canvas ; q,f of type boolean begin
{- Asignar al contador 
el valor 35. -}
contador:=0;
z:=7; 
with j from -z to z repeat
using j of type canvas begin
q :=5 ;
contador:=contador+1;
with j from z to z+2 repeat
using j of type integer begin
j:=55
end
done
end
done
end
